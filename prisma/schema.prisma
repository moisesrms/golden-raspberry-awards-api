generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:memorydb?mode=memory&cache=shared"
}

model Movie {
  id        String  @id @default(cuid())
  year      Int
  title     String
  studios   String
  producers String
  winner    Boolean @default(false)
}
